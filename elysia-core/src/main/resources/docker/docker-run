docker run --name nacos1 -p 8848:8848 -p 9848:9848 -p 9849:9849 -d --privileged=true --restart=always -e PREFER_HOST_MODE=hostname -e SPRING_DATASOURCE_PLATFORM=mysql -e MYSQL_SERVICE_HOST=192.168.17.133 -e MYSQL_SERVICE_PORT=3306 -e MYSQL_SERVICE_DB_NAME=nacos_config -e MYSQL_SERVICE_USER=root -e MYSQL_SERVICE_PASSWORD=123456 -v /home/elysiakafka/nacos1/logs:/home/nacos/logs -v /home/elysiakafka/nacos1/init.d/custom.properties:/etc/nacos/init.d/custom.properties -v /home/elysiakafka/nacos1/data:/home/nacos/data -v /home/elysiakafka/nacos1/conf:/home/nacos/conf nacos/nacos-server

