docker run --name mysql -p 3306:3306 -d --restart=always -e MYSQL_ROOT_PASSWORD=123456 -v /home/elysiakafka/mysql/conf:/etc/mysql -v /home/elysiakafka/mysql/data:/var/lib/mysql -v /home/elysiakakfa/mysql/log:/var/log/mysql mysql

docker run --name nginx -p 80:80 -d --restart=always -v /home/elysiakafka/nginx/nginx.conf:/etc/nginx/nginx.conf -v /home/elysiakafka/nginx/html:/usr/share/nginx/html nginx
